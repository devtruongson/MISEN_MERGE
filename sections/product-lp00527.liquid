        <div class="container mx-auto px-4">
          <div class="flex items-center justify-center gap-3 text-sm sm:text-base">
            <span class="flex items-center gap-2">
              <i
                class="fas fa-{{ template_data.banner_icon_1 }}"
                data-meta="banner_icon_1"
                data-type="icon"
              ></i>
              <span class="font-medium" data-meta="banner_text_1">{{ template_data.banner_text_1 }}</span>
            </span>
            <span class="hidden sm:flex items-center gap-2 text-emerald-400">
              <i
                class="fas fa-{{ template_data.banner_icon_2 }}"
                data-meta="banner_icon_2"
                data-type="icon"
              ></i>
              <span id="countdown-timer" data-meta="countdown_timer_1">{{ template_data.countdown_timer_1 }}</span>
            </span>
          </div>
        </div>
      </div>

      <nav class="container mx-auto px-4">
        <div class="flex items-center justify-between h-16 lg:h-20">
          <div class="flex items-center gap-3">
            <div class="flex items-center">
              <span
                class="font-space font-bold text-2xl sm:text-3xl text-slate-900"
                data-meta="brand_part_1"
              >
                {{- template_data.brand_part_1 -}}
              </span>
              <span
                class="font-space font-bold text-2xl sm:text-3xl gradient-text"
                data-meta="brand_part_2"
              >
                {{- template_data.brand_part_2 -}}
              </span>
            </div>
            <span
              class="inline-block bg-emerald-500 text-white text-xs font-bold px-2.5 py-1 rounded"
              data-meta="brand_badge"
            >
              {{ template_data.brand_badge }}
            </span>
          </div>

          <div class="hidden lg:flex items-center gap-8">
            <a
              href="{{ template_data.nav_link_1 }}"
              class="text-slate-600 hover:text-emerald-600 font-medium transition-colors"
              data-meta="nav_link_1 nav_text_1"
            >
              {{- template_data.nav_text_1 -}}
            </a>
            <a
              href="{{ template_data.nav_link_2 }}"
              class="text-slate-600 hover:text-emerald-600 font-medium transition-colors"
              data-meta="nav_link_2 nav_text_2"
            >
              {{- template_data.nav_text_2 -}}
            </a>
            <a
              href="{{ template_data.nav_link_3 }}"
              class="text-slate-600 hover:text-emerald-600 font-medium transition-colors"
              data-meta="nav_link_3 nav_text_3"
            >
              {{- template_data.nav_text_3 -}}
            </a>
            <a
              href="{{ template_data.nav_link_4 }}"
              class="text-slate-600 hover:text-emerald-600 font-medium transition-colors"
              data-meta="nav_link_4 nav_text_4"
            >
              {{- template_data.nav_text_4 -}}
            </a>
            <a
              href="{{ template_data.nav_link_5 }}"
              class="bg-gradient-to-r from-emerald-600 to-green-600 text-white font-semibold px-6 py-2.5 rounded-full hover:shadow-lg transition-all transform hover:scale-105"
              data-meta="nav_link_5 nav_cta_text"
            >
              {{ template_data.nav_cta_text }}
            </a>
          </div>

          <button id="menu-toggle" class="lg:hidden p-2">
            <i class="fas fa-{{ template_data.menu_icon }}" data-meta="menu_icon" data-type="icon"></i>
          </button>
        </div>

        <div id="mobile-nav" style="display: none" class="lg:hidden pb-4 border-t border-slate-200 mt-2 pt-4">
          <div class="flex flex-col gap-3">
            <a
              href="{{ template_data.mobile_nav_link_1 }}"
              class="text-slate-600 font-medium py-2"
              data-meta="mobile_nav_link_1 mobile_nav_text_1"
            >
              {{- template_data.mobile_nav_text_1 -}}
            </a>
            <a
              href="{{ template_data.mobile_nav_link_2 }}"
              class="text-slate-600 font-medium py-2"
              data-meta="mobile_nav_link_2 mobile_nav_text_2"
            >
              {{- template_data.mobile_nav_text_2 -}}
            </a>
            <a
              href="{{ template_data.mobile_nav_link_3 }}"
              class="text-slate-600 font-medium py-2"
              data-meta="mobile_nav_link_3 mobile_nav_text_3"
            >
              {{- template_data.mobile_nav_text_3 -}}
            </a>
            <a
              href="{{ template_data.mobile_nav_link_4 }}"
              class="text-slate-600 font-medium py-2"
              data-meta="mobile_nav_link_4 mobile_nav_text_4"
            >
              {{- template_data.mobile_nav_text_4 -}}
            </a>
            <a
              href="{{ template_data.mobile_nav_link_5 }}"
              class="bg-emerald-600 text-white font-semibold text-center py-3 rounded-full"
              data-meta="mobile_nav_link_5 mobile_nav_cta_text"
            >
              {{ template_data.mobile_nav_cta_text }}
            </a>
          </div>
        </div>
      </nav>
    </header>

    <section
      class="min-h-screen flex items-center relative overflow-hidden bg-gradient-to-b from-white via-emerald-50/20 to-white py-12 lg:py-16"
    >
      <div class="absolute inset-0 opacity-5">
        <div class="absolute top-0 left-0 w-96 h-96 bg-emerald-500 rounded-full blur-3xl"></div>
        <div class="absolute bottom-0 right-0 w-96 h-96 bg-green-500 rounded-full blur-3xl"></div>
      </div>

      <div class="container mx-auto px-4 sm:px-6 lg:px-8 relative z-10">
        <div class="hero-container">
          <div class="hero-badges animate-fadeInUp">
            <div class="inline-flex items-center gap-3">
              <div
                class="flex items-center gap-2 bg-white border border-slate-200 px-4 py-2 rounded-full shadow-sm"
              >
                <div class="flex -space-x-2">
                  <span class="inline-block w-2 h-2 bg-green-500 rounded-full"></span>
                  <span class="inline-block w-2 h-2 bg-green-500 rounded-full"></span>
                  <span class="inline-block w-2 h-2 bg-green-500 rounded-full"></span>
                </div>
                <span class="text-sm font-medium text-slate-700" data-meta="active_users_text">
                  {{- template_data.active_users_text -}}
                </span>
              </div>
            </div>
          </div>

          <h1
            class="hero-headline font-space font-bold text-4xl sm:text-5xl lg:text-6xl xl:text-7xl text-slate-900 leading-[1.1] animate-fadeInUp"
          >
            <span data-meta="headline_part_1">{{ template_data.headline_part_1 }}</span>
            <span class="relative inline-block">
              <span
                class="relative z-10 text-transparent bg-clip-text bg-gradient-to-r from-emerald-600 to-green-500"
                data-meta="headline_highlight"
              >
                {{ template_data.headline_highlight }}
              </span>
              <span class="absolute bottom-2 left-0 w-full h-3 bg-emerald-200 opacity-30"></span>
            </span>
            <br>
            <span data-meta="headline_part_2">{{ template_data.headline_part_2 }}</span>
          </h1>

          <p
            class="hero-subheadline text-lg sm:text-xl lg:text-2xl text-slate-600 leading-relaxed animate-fadeInUp"
            data-meta="hero_subheadline"
          >
            {{ template_data.hero_subheadline }}
          </p>

          <div class="hero-image animate-fadeInUp">
            <div class="relative rounded-2xl overflow-hidden shadow-2xl">
              <img
                src="{{ template_data.hero_image }}"
                alt=""
                data-meta="hero_image"
                data-type="image"
                class="w-full h-auto object-cover"
              >

              <div
                class="absolute top-4 sm:top-6 left-4 sm:left-6 bg-black/80 text-white px-3 sm:px-4 py-1.5 sm:py-2 rounded-full text-sm font-medium backdrop-blur-sm"
              >
                <i
                  class="fas fa-{{ template_data.hero_badge_icon }}"
                  data-meta="hero_badge_icon"
                  data-type="icon"
                ></i>
                <span class="hidden sm:inline" data-meta="hero_badge_text_desktop">
                  {{- template_data.hero_badge_text_desktop -}}
                </span>
                <span class="sm:hidden" data-meta="hero_badge_text_mobile">
                  {{- template_data.hero_badge_text_mobile -}}
                </span>
              </div>

              <div class="absolute bottom-4 sm:bottom-6 left-4 sm:left-6 right-4 sm:right-auto">
                <div
                  class="bg-white/95 backdrop-blur-md rounded-xl p-3 sm:p-4 shadow-xl flex items-center gap-3 max-w-xs"
                >
                  <div
                    class="w-10 h-10 sm:w-12 sm:h-12 bg-emerald-100 rounded-lg flex items-center justify-center flex-shrink-0"
                  >
                    <i
                      class="fas fa-{{ template_data.hero_feature_icon }}"
                      data-meta="hero_feature_icon"
                      data-type="icon"
                    ></i>
                  </div>
                  <div>
                    <p
                      class="font-semibold text-slate-900 text-sm sm:text-base"
                      data-meta="hero_feature_title"
                    >
                      {{ template_data.hero_feature_title }}
                    </p>
                    <p class="text-xs sm:text-sm text-slate-600" data-meta="hero_feature_subtitle">
                      {{ template_data.hero_feature_subtitle }}
                    </p>
                  </div>
                </div>
              </div>
            </div>
          </div>

          <div class="hero-features grid grid-cols-2 sm:grid-cols-4 gap-4 animate-fadeInUp">
            <div class="flex flex-col items-center text-center space-y-2">
              <div class="w-12 h-12 bg-emerald-100 rounded-xl flex items-center justify-center">
                <i
                  class="fas fa-{{ template_data.feature_icon_1 }}"
                  data-meta="feature_icon_1"
                  data-type="icon"
                ></i>
              </div>
              <span class="text-sm font-medium text-slate-700" data-meta="feature_text_1">
                {{- template_data.feature_text_1 -}}
              </span>
            </div>
            <div class="flex flex-col items-center text-center space-y-2">
              <div class="w-12 h-12 bg-emerald-100 rounded-xl flex items-center justify-center">
                <i
                  class="fas fa-{{ template_data.feature_icon_2 }}"
                  data-meta="feature_icon_2"
                  data-type="icon"
                ></i>
              </div>
              <span class="text-sm font-medium text-slate-700" data-meta="feature_text_2">
                {{- template_data.feature_text_2 -}}
              </span>
            </div>
            <div class="flex flex-col items-center text-center space-y-2">
              <div class="w-12 h-12 bg-emerald-100 rounded-xl flex items-center justify-center">
                <i
                  class="fas fa-{{ template_data.feature_icon_3 }}"
                  data-meta="feature_icon_3"
                  data-type="icon"
                ></i>
              </div>
              <span class="text-sm font-medium text-slate-700" data-meta="feature_text_3">
                {{- template_data.feature_text_3 -}}
              </span>
            </div>
            <div class="flex flex-col items-center text-center space-y-2">
              <div class="w-12 h-12 bg-emerald-100 rounded-xl flex items-center justify-center">
                <i
                  class="fas fa-{{ template_data.feature_icon_4 }}"
                  data-meta="feature_icon_4"
                  data-type="icon"
                ></i>
              </div>
              <span class="text-sm font-medium text-slate-700" data-meta="feature_text_4">
                {{- template_data.feature_text_4 -}}
              </span>
            </div>
          </div>

          <div class="hero-cta space-y-6 animate-fadeInUp">
            <div class="flex flex-col sm:flex-row gap-4">
              <a
                href="{{ template_data.cta_primary_link }}"
                class="flex-1 bg-gradient-to-r from-emerald-600 to-green-600 hover:from-emerald-700 hover:to-green-700 text-white font-bold text-center py-4 px-6 rounded-xl shadow-lg hover:shadow-xl transition-all transform hover:-translate-y-0.5"
                data-meta="cta_primary_link"
              >
                <i
                  class="fas fa-{{ template_data.cta_primary_icon }}"
                  data-meta="cta_primary_icon"
                  data-type="icon"
                ></i>
                <span data-meta="cta_primary_text">{{ template_data.cta_primary_text }}</span>
              </a>
              <button
                onclick="playDemo()"
                class="sm:flex-1 bg-white hover:bg-slate-50 text-slate-700 font-semibold text-center py-4 px-6 rounded-xl border-2 border-slate-200 transition-colors"
              >
                <i
                  class="fas fa-{{ template_data.cta_secondary_icon }}"
                  data-meta="cta_secondary_icon"
                  data-type="icon"
                ></i>
                <span data-meta="cta_secondary_text">{{ template_data.cta_secondary_text }}</span>
              </button>
            </div>

            <div class="flex flex-wrap items-center justify-center gap-x-6 gap-y-2 text-sm text-slate-600">
              <span class="flex items-center gap-1.5">
                <i
                  class="fas fa-{{ template_data.trust_icon_1 }}"
                  data-meta="trust_icon_1"
                  data-type="icon"
                ></i>
                <span data-meta="trust_text_1">{{ template_data.trust_text_1 }}</span>
              </span>
              <span class="flex items-center gap-1.5">
                <i
                  class="fas fa-{{ template_data.trust_icon_2 }}"
                  data-meta="trust_icon_2"
                  data-type="icon"
                ></i>
                <span data-meta="trust_text_2">{{ template_data.trust_text_2 }}</span>
              </span>
              <span class="flex items-center gap-1.5">
                <i
                  class="fas fa-{{ template_data.trust_icon_3 }}"
                  data-meta="trust_icon_3"
                  data-type="icon"
                ></i>
                <span data-meta="trust_text_3">{{ template_data.trust_text_3 }}</span>
              </span>
            </div>
          </div>

          <div
            class="hero-social flex flex-wrap items-center justify-center lg:justify-start gap-4 pt-6 border-t border-slate-200 animate-fadeInUp"
          >
            <div class="flex">
              <i class="fas fa-star text-yellow-400"></i>
              <i class="fas fa-star text-yellow-400"></i>
              <i class="fas fa-star text-yellow-400"></i>
              <i class="fas fa-star text-yellow-400"></i>
              <i class="fas fa-star text-yellow-400"></i>
            </div>
            <div>
              <span class="font-bold text-slate-900" data-meta="rating_score">{{ template_data.rating_score }}</span>
              <span class="text-sm text-slate-600 ml-1" data-meta="rating_text">{{ template_data.rating_text }}</span>
            </div>
          </div>
        </div>

        <div class="flex justify-center mt-12 lg:mt-16 animate-bounce">
          <button
            onclick="scrollToNext()"
            class="text-center text-slate-400 hover:text-emerald-600 transition-colors"
          >
            <p class="text-sm font-medium mb-2" data-meta="scroll_text">{{ template_data.scroll_text }}</p>
            <i class="fas fa-{{ template_data.scroll_icon }}" data-meta="scroll_icon" data-type="icon"></i>
          </button>
        </div>
      </div>
    </section>

    <section id="technology" class="py-20 lg:py-28 bg-white">
      <div class="container mx-auto px-4 sm:px-6 lg:px-8 max-w-5xl">
        <div class="text-center mb-14 lg:mb-16">
          <span
            class="inline-block bg-emerald-100 text-emerald-700 px-4 py-2 rounded-full text-sm font-semibold mb-5"
            data-meta="tech_section_label"
          >
            {{ template_data.tech_section_label }}
          </span>
          <h2 class="font-space font-bold text-3xl sm:text-4xl lg:text-5xl text-slate-900 mb-6">
            <span data-meta="tech_headline_1">{{ template_data.tech_headline_1 }}</span>
            <span class="gradient-text" data-meta="tech_headline_2"> {{ template_data.tech_headline_2 }}</span>
          </h2>
          <p class="text-lg sm:text-xl text-slate-600 max-w-2xl mx-auto" data-meta="tech_subheadline">
            {{ template_data.tech_subheadline }}
          </p>
        </div>

        <div class="grid md:grid-cols-2 gap-8 lg:gap-10">
          <div class="group">
            <div class="flex items-start gap-4">
              <div
                class="w-14 h-14 bg-gradient-to-br from-emerald-500 to-green-600 rounded-2xl flex items-center justify-center flex-shrink-0 group-hover:scale-110 transition-transform"
              >
                <i
                  class="fas fa-{{ template_data.tech_feature_icon_1 }}"
                  data-meta="tech_feature_icon_1"
                  data-type="icon"
                ></i>
              </div>
              <div>
                <h3 class="font-semibold text-xl text-slate-900 mb-2" data-meta="tech_feature_title_1">
                  {{ template_data.tech_feature_title_1 }}
                </h3>
                <p class="text-slate-600 leading-relaxed" data-meta="tech_feature_desc_1">
                  {{ template_data.tech_feature_desc_1 }}
                </p>
              </div>
            </div>
          </div>

          <div class="group">
