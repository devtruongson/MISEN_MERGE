{% comment %} Template: VoiceAI Pro X1 - Professional AI Voice Recorder {% endcomment %}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ template_data.page_title }}</title>
    
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Manrope:wght@400;500;600;700;800&family=JetBrains+Mono:wght@500;600&display=swap" rel="stylesheet">
    
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css">
    
    <script src="https://cdn.tailwindcss.com"></script>
    
    <style>
        * {
            font-family: 'Manrope', sans-serif;
        }

        html {
            font-size: unset !important;
        }

        body {
            font-size: unset !important;
        }
        
        body {
            font-size: 18px;
            line-height: 1.7;
        }
        
        .mono-text {
            font-family: 'JetBrains Mono', monospace;
            font-size: 14px;
        }
        
        .gradient-text {
            background: linear-gradient(135deg, #10b981 0%, #f59e0b 100%);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
            background-clip: text;
        }
        
        .dark-gradient {
            background: linear-gradient(135deg, #0f172a 0%, #1e293b 100%);
        }
        
        .emerald-glow {
            box-shadow: 0 0 40px rgba(16, 185, 129, 0.2);
        }
        
        @keyframes slide-in {
            from { 
                opacity: 0;
                transform: translateX(-20px);
            }
            to { 
                opacity: 1;
                transform: translateX(0);
            }
        }
        
        .slide-in {
            animation: slide-in 0.6s ease-out;
        }
        
        @keyframes float-smooth {
            0%, 100% { transform: translateY(0); }
            50% { transform: translateY(-15px); }
        }
        
        .float-smooth {
            animation: float-smooth 5s ease-in-out infinite;
        }
        
        #reading-progress {
            position: fixed;
            top: 0;
            left: 0;
            height: 2px;
            background: #10b981;
            z-index: 9999;
            transition: width 0.2s linear;
        }
        
        .modern-card {
            background: white;
            border: 1px solid #e5e7eb;
            transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
            padding: 2rem;
        }
        
        .modern-card:hover {
            transform: translateY(-4px);
            box-shadow: 0 20px 40px rgba(0, 0, 0, 0.1);
            border-color: #10b981;
        }
        
        .minimal-notification {
            position: fixed;
            bottom: 30px;
            left: 30px;
            background: #0f172a;
            color: white;
            padding: 20px 24px;
            border-radius: 8px;
            box-shadow: 0 10px 30px rgba(0, 0, 0, 0.3);
            max-width: 360px;
            transform: translateX(-400px);
            transition: transform 0.4s ease;
            z-index: 1000;
            border-left: 3px solid #10b981;
        }
        
        .minimal-notification.show {
            transform: translateX(0);
        }
        
        .minimal-popup {
            display: none;
            position: fixed;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background: rgba(0, 0, 0, 0.9);
            z-index: 10000;
            justify-content: center;
            align-items: center;
        }
        
        .minimal-popup.active {
            display: flex;
        }
        
        .cta-primary {
            background: #10b981;
            color: white;
            transition: all 0.3s ease;
            position: relative;
            overflow: hidden;
            font-size: 18px;
            padding: 16px 32px;
        }
        
        .cta-primary:hover {
            background: #059669;
            transform: translateY(-2px);
            box-shadow: 0 10px 25px rgba(16, 185, 129, 0.3);
        }
        
        .cta-primary::after {
            content: '';
            position: absolute;
            top: 50%;
            left: 50%;
            width: 0;
            height: 0;
            border-radius: 50%;
            background: rgba(255, 255, 255, 0.2);
            transform: translate(-50%, -50%);
            transition: width 0.6s, height 0.6s;
        }
        
        .cta-primary:hover::after {
            width: 300px;
            height: 300px;
        }
        
        .cta-secondary {
            background: transparent;
            border: 2px solid #0f172a;
            color: #0f172a;
            transition: all 0.3s ease;
            font-size: 18px;
            padding: 16px 32px;
        }
        
        .cta-secondary:hover {
            background: #0f172a;
            color: white;
            transform: translateY(-2px);
        }
        
        .dot-pattern {
            background-image: radial-gradient(circle, #e5e7eb 1px, transparent 1px);
            background-size: 20px 20px;
        }
        
        .tech-badge {
            background: #f59e0b;
            color: #0f172a;
            font-weight: 600;
            padding: 6px 14px;
            border-radius: 4px;
            font-size: 12px;
            letter-spacing: 0.5px;
            display: inline-block;
        }
        
        .rotate-180 {
            transform: rotate(180deg);
        }

        section {
            padding-top: 6rem;
            padding-bottom: 6rem;
        }
        
        p {
            margin-bottom: 1.5rem;
        }
        
        h1, h2, h3, h4 {
            margin-bottom: 1.5rem;
            line-height: 1.3;
        }
    </style>

    <script>
      window.productData = {
            id: '{{ product.id }}',
            title: '{{ product.title | escape }}',
            handle: '{{ product.handle }}',
            price: {{ product.price }},
            compare_at_price: {% if product.compare_at_price %}{{ product.compare_at_price }}{% else %}''{% endif %},
            available: {{ product.available }},
            image: "{{ product.featured_image | image_url: width: 100 }}",
            variants: [
              {% for variant in product.variants %}
              {
                id: {{ variant.id }},
                title: "{{ variant.title | escape }}",
                price: {{ variant.price }},
                available: {{ variant.available }},
                quantity: {{ variant.inventory_quantity }},
                image: {% if variant.image %}"{{ variant.image | image_url: width: 100 }}"{% else %}""{% endif %}
              }{% unless forloop.last %},{% endunless %}
              {% endfor %}
            ],
            options: {{ product.options_with_values | json }},
            selected_or_first_available_variant: {{ product.selected_or_first_available_variant | json }},
            featuredImage: {
              url: '{{ product.featured_image | image_url: width: 1000 }}'
            },
            collections: {{ product.collections | json }},
            brandName: '{{ product.vendor | escape }}',
            productCategory: '{{ product.type | escape }}',
            productType: '{{ product.metafields.custom.product_type | escape }}',
            mainUSP: '{{ product.metafields.custom.usp | escape }}',
            productSize: '{{ product.metafields.custom.size | escape }}',
            targetCustomer: '{{ product.metafields.custom.target | escape }}',
            priceRange: '{{ product.metafields.custom.price_range | escape }}',
            availableReferenceImages: {{ product.metafields.custom.reference_images | json }}
          };
    </script>
    {% assign template_data = product.metafields.custom.template_data.value | parse_json %}
</head>
