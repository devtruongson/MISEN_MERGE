    <script>
        let currentGalleryImage = 0;
        const galleryImages = document.querySelectorAll('.gallery-image');
        const galleryDots = document.querySelectorAll('.gallery-dot');
        
        function updateGallery(index) {
            galleryImages.forEach(img => img.classList.add('hidden'));
            galleryImages[index].classList.remove('hidden');
            
            galleryDots.forEach((dot, i) => {
                if (i === index) {
                    dot.classList.remove('bg-white/50');
                    dot.classList.add('bg-white');
                } else {
                    dot.classList.remove('bg-white');
                    dot.classList.add('bg-white/50');
                }
            });
            
            currentGalleryImage = index;
        }
        
        document.getElementById('gallery-prev').addEventListener('click', () => {
            const newIndex = currentGalleryImage === 0 ? galleryImages.length - 1 : currentGalleryImage - 1;
            updateGallery(newIndex);
        });
        
        document.getElementById('gallery-next').addEventListener('click', () => {
            const newIndex = currentGalleryImage === galleryImages.length - 1 ? 0 : currentGalleryImage + 1;
            updateGallery(newIndex);
        });
        
        galleryDots.forEach((dot, index) => {
            dot.addEventListener('click', () => updateGallery(index));
        });
        
        let touchStartX = 0;
        let touchEndX = 0;
        const galleryContainer = document.getElementById('gallery-container');
        
        galleryContainer.addEventListener('touchstart', e => {
            touchStartX = e.changedTouches[0].screenX;
        });
        
        galleryContainer.addEventListener('touchend', e => {
            touchEndX = e.changedTouches[0].screenX;
            handleSwipe();
        });
        
        function handleSwipe() {
            if (touchEndX < touchStartX - 50) {
                const newIndex = currentGalleryImage === galleryImages.length - 1 ? 0 : currentGalleryImage + 1;
                updateGallery(newIndex);
            }
            if (touchEndX > touchStartX + 50) {
                const newIndex = currentGalleryImage === 0 ? galleryImages.length - 1 : currentGalleryImage - 1;
                updateGallery(newIndex);
            }
        }
        
        document.getElementById('mobile-menu-btn').addEventListener('click', function() {
            document.getElementById('mobile-menu').classList.toggle('hidden');
        });
        
        window.addEventListener('scroll', function() {
            const winScroll = document.body.scrollTop || document.documentElement.scrollTop;
            const height = document.documentElement.scrollHeight - document.documentElement.clientHeight;
            const scrolled = (winScroll / height) * 100;
            document.getElementById('reading-progress').style.width = scrolled + '%';
            
            if (scrolled > 30) {
                document.getElementById('sticky-cta').classList.remove('hidden');
            } else {
                document.getElementById('sticky-cta').classList.add('hidden');
            }
        });
        
        document.querySelectorAll('a[href^="#offer"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({ behavior: 'smooth' });
                }
            });
        });
        
        function showNotification(id) {
            const notification = document.getElementById(id);
            setTimeout(() => {
                notification.classList.add('show');
                setTimeout(() => {
                    notification.classList.remove('show');
                }, 4000);
            }, Math.random() * 5000 + 8000);
        }
        
        showNotification('notification-1');
        setTimeout(() => showNotification('notification-2'), 15000);
        
        setInterval(() => {
            showNotification('notification-1');
            setTimeout(() => showNotification('notification-2'), 15000);
        }, 35000);
        
        let exitShown = false;
        document.addEventListener('mouseout', function(e) {
            if (e.clientY <= 0 && !exitShown) {
                document.getElementById('exit-popup').classList.add('active');
                exitShown = true;
            }
        });
        
        document.getElementById('close-popup').addEventListener('click', function() {
            document.getElementById('exit-popup').classList.remove('active');
        });
        
        document.getElementById('claim-discount').addEventListener('click', function() {
            document.getElementById('popup-step-1').classList.add('hidden');
            document.getElementById('popup-step-2').classList.remove('hidden');
        });
        
        document.getElementById('get-code').addEventListener('click', function() {
            const email = document.getElementById('discount-email').value;
            if (email && email.includes('@')) {
                document.getElementById('popup-step-2').classList.add('hidden');
                document.getElementById('popup-step-3').classList.remove('hidden');
            }
        });
        
        document.getElementById('copy-code').addEventListener('click', function() {
            const code = document.getElementById('discount-code').textContent;
            navigator.clipboard.writeText(code);
            this.innerHTML = 'Copied!';
            setTimeout(() => {
                document.getElementById('exit-popup').classList.remove('active');
            }, 1500);
        });
        document.querySelectorAll('.faq-toggle').forEach(button => {
            button.addEventListener('click', function() {
                const content = this.nextElementSibling;
                const icon = this.querySelector('i');
                
                content.classList.toggle('hidden');
                icon.classList.toggle('rotate-180');
            });
        });
        
        function openPopup(popupId) {
            document.getElementById(popupId).style.display = 'flex';
            document.body.style.overflow = 'hidden';
        }
        
        function closePopup(popupId) {
            document.getElementById(popupId).style.display = 'none';
            document.body.style.overflow = 'auto';
        }
        
        document.getElementById('privacy-link').addEventListener('click', function(e) {
            e.preventDefault();
            openPopup('privacy-popup');
        });
        
        document.getElementById('terms-link').addEventListener('click', function(e) {
            e.preventDefault();
            openPopup('terms-popup');
        });
        
        document.getElementById('disclaimer-link').addEventListener('click', function(e) {
            e.preventDefault();
            openPopup('disclaimer-popup');
        });
        
        document.getElementById('about-link').addEventListener('click', function(e) {
            e.preventDefault();
            openPopup('about-popup');
        });
        
        document.querySelectorAll('#privacy-popup, #terms-popup, #disclaimer-popup, #about-popup').forEach(popup => {
            popup.addEventListener('click', function(e) {
                if (e.target === this) {
                    this.style.display = 'none';
                    document.body.style.overflow = 'auto';
                }
            });
        });
    </script>
